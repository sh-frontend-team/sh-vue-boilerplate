{
    "presets": [["env", "babel-preset-env"], "stage-3"],
    "plugins": [
        "dynamic-import-node",
        "transform-class-properties",
        "transform-es5-property-mutators",
        "transform-object-rest-spread",
        "transform-runtime",
        "transform-vue-jsx",
        [
            "import",
            {
                "libraryName": "vant",
                "libraryDirectory": "es",
                "style": true
            }
        ]
    ],
    "comments": false
}
